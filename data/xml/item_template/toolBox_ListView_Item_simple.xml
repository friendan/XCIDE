<?xml version="1.0" encoding="UTF-8"?>
<!--炫彩界面库-列表视元素项模板文件-->
<head>
	<bindJsFile value="" />
</head>
<xc_template_listView showT="true">
	<listView_Group showT="true" expandT="true">
		<layoutEleUI drawBorder="false" enableCanvas="false" heightT="30" layout.alignH="left" layout.alignV="top" layout.height="fill" layout.horizon="true" layout.space="0" layout.width="fill" mouseThrough="true" padding="5,0,0,0" showT="true" widthT="200" expandT="true">
			<buttonUI buttonStyle="expand" buttonType="check" expandButton="true" itemID="1" layout.height="fill" showT="true" width="16" expandT="true" />
			<shapeText content="{binding field=name3 source=data bSync=false}" css.name="style_shapeText" itemID="2" layout.height="fill" layout.width=":1" showT="true" textColor="000000000" expandT="true" />
		</layoutEleUI>
	</listView_Group>
	<listView_item showT="true" expandT="true">
		<layoutEleUI drawBorder="false" enableCanvas="false" heightT="30" layout.alignH="center" layout.alignV="center" layout.height="fill" layout.horizon="true" layout.space="0" layout.width="fill" mouseThrough="true" padding="0,0,0,0" showT="true" widthT="30" expandT="true">
			<shapePicture image="{binding field=name2 source=data bSync=false}" itemID="1" rect="0,0,20,20" showT="true" expandT="true" />
		</layoutEleUI>
	</listView_item>
</xc_template_listView>
